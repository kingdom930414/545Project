 <header class="header black-bg">
      <!--logo start-->
      <a href="/" class="logo"><b>On<span>Campus</span></b></a>
      <!--logo end-->
      <div class="top-menu">
        <ul class="nav pull-right top-menu">
          <li><a class="logout" href="/">Sign Out</a></li>
        </ul>
      </div>
</header>
    
      <div class="container-fluid">
          {{#each groups}}
          <div class="row mb-2 wrap">
            <div class="col-md-12">
              <a href="/{{this._id}}" >
                <div class="card-body">
                  <div class="row">
                    <div class="col-xs-1 col-sm-2 col-md-1 mr-3 cardlocater">
                      <br>
                    </div>
                    <div class="col-xs-16 col-sm-16 col-md-16 cardlocater">
                      <h1 class="article-title">{{this.name}}</h1>
                      <p class='article-description'>This is a group for {{this.name}}</p>
                    </div>
                  </div>
                </div>
              </a>
            </div>
          </div>

          {{/each}}
      </div>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>